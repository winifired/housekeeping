.logo {
  width: 176rpx;
  height: 176rpx;
  margin: 0 auto;
}

.item {
  padding: 55rpx 0 20rpx;
  width: 612rpx;
  margin: 0 auto;
  border-bottom: 1px solid #DDDDDD;
}

.item>view {
  font-size: 32rpx;
  font-weight: 500;
  color: #A7B4B1;
}

.item>input {
  font-size: 32rpx;
  font-weight: 500;
  color: #011F18;
  flex: 1;
}

.phone {
  text-align: center;
}

.save {
  width: 580rpx;
  margin: 117rpx auto 54rpx;
}

.tip {
  font-size: 24rpx;
  font-weight: 500;
  color: #A9A8A8;
}

.tip>view {
  color: #35BB9A !important;
}

.winxin {
  width: 108rpx;
  height: 108rpx;
  margin:250rpx auto;
  /* position: fixed;
  bottom: 108rpx;
  left: 50%;
  transform: translateX(-50%); */
}
.winxin>image{
  width:100%;
  height: 100%;
}
button::after{
  border:none;
}
.login {
  padding-top: 100rpx;
  padding-bottom: env(safe-area-inset-bottom);
}

page {
  height: auto;
  background: #fff;
}

.model {
  width: 582rpx;
  background: #FFFFFF;
  border-radius: 30rpx;
}

.title {
  font-size: 38rpx;
  text-align: center;
  height: 120rpx;
  line-height: 120rpx;
  font-weight: 500;
  color: #02261D;
}

.content {
  font-size: 28rpx;
  font-weight: 400;
  color: #697673;
  line-height: 42rpx;
  height: 600rpx;
  overflow-y: scroll;
  padding: 0 32rpx;
}
.btn{
  border-top:1px solid #EEEFF2;
}
.btn>view {
  width: 50%;
  text-align: center;
  height: 101rpx;
  line-height: 101rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #35BB9A;
}
.img_rich {
  width: 100%!important;
  height: auto;
  display: block;
  margin: 20rpx 0;
}

.richText col {
  width: 100%!important;
}
.table_rich{
  width:100%!important;
}